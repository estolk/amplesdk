<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../../../stylesheets/page.xsl"?>
<class name="AMLTouchList">
	<abstract>
		<locale>
			The <link type="class">AMLTouchList</link> class is used to represent a collection of <link type="class">AMLTouch</link> objects.
		</locale>
	</abstract>
	<examples/>
	<members>
		<properties>
			<property name="length" type="Number" readonly="true">
				<description>
					<locale>
						The number of <link type="class">AMLTouch</link> objects in this <link type="class">AMLTouchList</link> object.
					</locale>
				</description>
			</property>
		</properties>
		<methods>
			<method name="item" type="AMLTouch">
				<arguments>
					<argument name="index" type="Number">
						<description>
							<locale>
								The index of the item to return.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Returns the <link type="class">AMLTouch</link> object at the given index.
					</locale>
				</description>
			</method>
		</methods>
	</members>
</class>
